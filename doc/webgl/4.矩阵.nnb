                                "}\r",
                                "\r",
                                "// 点积函数 获取投影长度\r",
                                "function dot(a, b) {\r",
                                "  return a[0] * b[0] + a[1] * b[1] + a[2] * b[2];\r",
                                "}\r",
                                "\r",
                                "// 向量差\r",
                                "function minus(a, b) {\r",
