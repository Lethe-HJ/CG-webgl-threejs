                                "      A[i] * B[12] + A[i + 4] * B[13] + A[i + 8] * B[14] + A[i + 12] * B[15];\r",
                                "  }\r",
                                "\r",
                                "  return result;\r",
                                "}\r",
                                "\r",
                                "// 归一化函数\r",
                                "function normalized(arr) {\r",
                                "  let sum = 0;\r",
                                "\r",
                                "  for (let i = 0; i < arr.length; i++) {\r",
                                "    sum += arr[i] * arr[i];\r",
                                "  }\r",
                                "\r",
                                "  const middle = Math.sqrt(sum);\r",
                                "\r",
                                "  for (let i = 0; i < arr.length; i++) {\r",
                                "    arr[i] = arr[i] / middle;\r",
                                "  }\r",
